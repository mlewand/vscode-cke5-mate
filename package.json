{
	"name": "cke5-mate",
	"displayName": "CKEditor 5 mate",
	"description": "A set of helpers for developing CKEditor5 code.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:cke5-mate.jsdocSignature"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "cke5-mate.jsdocSignature",
				"title": "Get CKE5 JSDoc signature"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"chai": "^4.2.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	}
}
